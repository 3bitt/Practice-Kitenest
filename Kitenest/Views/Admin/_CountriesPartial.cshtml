@model IEnumerable<Kitenest.Models.Country>


<table class="table">
    
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-controller="Countries" asp-route-id="@item.Id">Edit</a> |

                <a asp-action="Delete" asp-controller="Countries" asp-route-id="@item.Id" style="color:red">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
